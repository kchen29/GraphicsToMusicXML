#include <iostream>
#include <fstream>
#include "Part.h"
#include "Measure.h"
#include "Note.h"
 
using namespace std;


int main(void) {
    Part p;
    Measure m;
    Note n;
        
    //Set values
   p.setPart(5);
   m.setNumber(7);
   n.setPitch(2,3);
   
    //Open file for output
   ofstream myfile;
   myfile.open ("test.xml");//, ios_base::app (for appending to file)
   myfile << "Measure number (11) is :" << m.getNumber() << "\n";
   myfile.close();
   return 0;
}
