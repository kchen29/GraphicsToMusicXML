//=================================
// include guard
#ifndef __PART_H_INCLUDED__
#define __PART_H_INCLUDED__

//=================================
// forward declared dependencies
#include "Measure.h"

class Part  {
   public:
      void setPart(int p) {
         part = p;
      }
      
       int getPart(){
        return part;
      }
		
   protected:
      int part;
      Measure m;
};

#endif // __PART_H_INCLUDED__ 
